{
    "name": "stonebreaker",
    "description": "A tool for projects",
    "version": "0.0.1",
    "license": "GPL-3.0",
    "author": "Francisco José Fuentes Rodríguez",
    "repository": {
        "type": "git",
        "url": "https://github.com/gargoris/stonebreaker.git"
    },
    "scripts": {
        "start": "webpack-dev-server --hot --inline",
        "build": "webpack -p --bail",
        "test": "run-p 'test:*'",
        "test:elm": "elm test --warn --compiler node_modules/.bin/elm-make elm/TestRunner.elm",
        "test:js": "cross-env NODE_ENV=test karma start --single-run",
        "tdd": "run-p 'tdd:*'",
        "tdd:elm": "nodemon --watch 'elm/' --ext elm --exec 'npm run test:elm'",
        "tdd:js": "cross-env NODE_ENV=test karma start",
        "clean": "rm -rf dist elm-stuff/build-artifacts",
        "elm": "elm",
        "elm-repl": "elm repl",
        "elm-package": "elm package",
        "elm-reactor": "elm reactor",
        "elm-make": "elm make",
        "elm-test": "elm test",
        "ncu": "ncu",
        "preversion": "run-s 'ncu -- --error-level 2' test",
        "version": "json -4 -I -f elm-package.json -e \"this.version='${npm_package_version}'\" && git add elm-package.json",
        "postversion": "git push && git push origin v${npm_package_version}",
        "postinstall": "elm package install -y"
    },
    "engines": {
        "node": ">= 6.0"
    },
    "devDependencies": {
        "autoprefixer": "^7.0.1",
        "babel-core": "^6.24.1",
        "babel-eslint": "^7.2.3",
        "babel-loader": "^7.0.0",
        "babel-plugin-istanbul": "^4.1.3",
        "babel-preset-es2015": "^6.24.1",
        "chai": "^3.5.0",
        "cross-env": "^5.0.0",
        "css-loader": "^0.28.1",
        "dirty-chai": "^1.2.2",
        "dotenv": "^4.0.0",
        "elm": "^0.18.0",
        "elm-css": "^0.6.0",
        "elm-css-webpack-loader": "^3.1.0",
        "elm-hot-loader": "^0.5.4",
        "elm-test": "^0.18.2",
        "elm-webpack-loader": "^4.3.1",
        "eslint": "^3.19.0",
        "eslint-loader": "^1.7.1",
        "extract-text-webpack-plugin": "^2.1.0",
        "file-loader": "^0.11.1",
        "html-webpack-plugin": "^2.28.0",
        "json": "^9.0.6",
        "karma": "^1.7.0",
        "karma-chai": "^0.1.0",
        "karma-chai-sinon": "^0.1.5",
        "karma-coverage": "^1.1.1",
        "karma-dirty-chai": "^1.0.2",
        "karma-mocha": "^1.3.0",
        "karma-mocha-reporter": "^2.2.3",
        "karma-phantomjs-launcher": "^1.0.4",
        "karma-sourcemap-loader": "^0.3.7",
        "karma-webpack": "^2.0.3",
        "mocha": "^3.3.0",
        "node-elm-compiler": "^4.3.1",
        "nodemon": "^1.11.0",
        "noop-loader": "^1.0.0",
        "npm-check-updates": "^2.11.1",
        "npm-run-all": "^4.0.2",
        "phantomjs-prebuilt": "^2.1.14",
        "postcss-loader": "^2.0.5",
        "sinon": "^2.2.0",
        "sinon-chai": "^2.10.0",
        "style-loader": "^0.17.0",
        "url-loader": "^0.5.8",
        "webpack": "^2.5.1",
        "webpack-dev-server": "^2.4.5"
    }
}